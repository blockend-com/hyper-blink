{
  "manifest_version": 3,
  "name": "React blinks",
  "version": "1.0.0",
  "action": {
    "default_title": "Popup",
    "default_popup": "index.html"
  },
  "icons": {},
  "permissions": ["storage", "activeTab"],
  "content_scripts": [
    {
      "matches": [
        "https://twitter.com/*",
        "https://x.com/*",
        "https://pro.x.com/*"
      ],
      "js": ["twitter.js", "inject.js"],
      "media": []
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["pageScript.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
